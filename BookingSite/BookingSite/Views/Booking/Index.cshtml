<link href="~/Content/themes/Styles/StyleSheetBooking.css" rel="stylesheet" type="text/css">
<script>
    function goToCreateBooking() { window.location.href = "/Booking/CreateBooking" };
    function deleteBooking() { };
</script>
<div class="main-content">
    <h1> Mine bookinger </h1>

    <table class="table" id="table">
        <tr>
            <th class="leftmost"><input type="checkbox" onclick="checkAllBoxes()" class="master-checkbox"/></th>
            <th>Fag</th>
            <th>Starttid</th>
            <th>Sluttid</th>
        </tr>
        @foreach (var e in ViewBag.BookingList)
        { 
            <tr>
                <td class="leftmost"><input type="checkbox" onclick="checkAllBoxes()" class="master-checkbox"/></td>
                <td>@e.Subject.Name</td>
                <td>@e.StartTime</td>
                <td>@e.EndTime</td>
            </tr>
        }
    </table>

    <input type ="button" onclick = "goToCreateBooking()" value = "Opret ny" />
    <input id="btn-delete" type ="button" onclick = "deleteBooking()" value="Slet" />
</div>