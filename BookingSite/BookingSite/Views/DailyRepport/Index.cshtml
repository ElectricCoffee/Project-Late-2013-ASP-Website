@{
    ViewBag.Title = "Index";
}

<h2>Dagsrapport</h2>

<form method="get"> 
        <div class="chose-date">
        <h2>Vælg dato</h2>
            <select name="date">
            @foreach (var date in ViewBag.Dates) {
                 <option value="@date"></option>
            }
            </select>
       </div>
</form>
<div class="repport-table">
    <form action="/DailyRepport/table">
        <table border="1">
            <tr>
                <th><input type="checkbox" onclick="checkAllBoxes()" class="master-checkbox"/></th>
                <th>Fornavn</th>
                <th>Efternavn</th>
                <th>Fag</th>
                <th>Tid</th>
            </tr>
    </form>
</div>